<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="ColorTrackTextView">
        <attr name="originalColor" format="color"/>
        <attr name="changeColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="PwdEditText">
        <attr name="space" format="dimension"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="checkedColor" format="color"/>
        <attr name="defaultColor" format="color"/>
        <attr name="textLength" format="integer"/>
        <attr name="backColor" format="color"/>
        <attr name="tColor" format="color"/>
        <attr name="circle" format="dimension"/>
        <attr name="round" format="dimension"/>
        <attr name="tSize" format="dimension"/>
        <attr name="isPwd" format="boolean"/>
        <attr name="waitInputColor" format="color"/>
        <attr name="isWaitInput" format="boolean"/>
    </declare-styleable>

    <!-- slide bar 有效内容背景 -->
    <attr name="backgroundColor" format="color|reference" />
    <!-- slide bar　描边 -->
    <attr name="strokeColor" format="color|reference" />
    <!-- slide bar 文字颜色 -->
    <attr name="textColor" format="color|reference" />
    <!-- slide bar　文字大小 -->
    <attr name="textSize" format="dimension" />
    <!-- slide bar 选中文字颜色 -->
    <attr name="selectTextColor" format="color|reference" />
    <!-- slide bar 选中文字大小 -->
    <attr name="selectTextSize" format="dimension" />
    <!-- slide bar　选中时候提示文字颜色 -->
    <attr name="hintTextColor" format="color|reference" />
    <!-- slide bar　选中时候提示文字大小 -->
    <attr name="hintTextSize" format="dimension" />
    <!-- slide bar　选中时候提示圆形半径 -->
    <attr name="hintCircleRadius" format="dimension" />
    <!-- slide bar　选中时候提示圆形颜色 -->
    <attr name="hintCircleColor" format="color|reference" />
    <!-- slide bar　选中时候提示背景形状 -->
    <attr name="hintShape" format="enum" >
        <enum name="circle" value="0" />
        <enum name="rectangle" value="1" />
    </attr>
    <!-- slide bar　选中时候波浪颜色 -->
    <attr name="waveColor" format="color|reference" />
    <!-- slide bar　选中时候波浪半径 -->
    <attr name="waveRadius" format="dimension" />
    <!-- slide bar　里面文字上＋下padding -->
    <attr name="contentPadding" format="dimension" />
    <!-- slide bar　上+下+右padding -->
    <attr name="barPadding" format="dimension" />
    <!-- slide bar　宽度 -->
    <attr name="barWidth" format="dimension" />


    <declare-styleable name="AZWaveSideBarView">
        <!-- slide bar 有效内容背景 -->
        <attr name="backgroundColor" />
        <!-- slide bar　描边 -->
        <attr name="strokeColor" />
        <!-- slide bar 文字颜色 -->
        <attr name="textColor" />
        <!-- slide bar　文字大小 -->
        <attr name="textSize" />
        <!-- slide bar 选中文字颜色 -->
        <attr name="selectTextColor" />
        <!-- slide bar 选中文字大小 -->
        <attr name="selectTextSize" />
        <!-- slide bar　选中时候提示文字颜色 -->
        <attr name="hintTextColor" />
        <!-- slide bar　选中时候提示文字大小 -->
        <attr name="hintTextSize" />
        <!-- slide bar　选中时候提示圆形半径 -->
        <attr name="hintCircleRadius" />
        <!-- slide bar　选中时候提示圆形颜色 -->
        <attr name="hintCircleColor" />
        <!-- slide bar　选中时候波浪颜色 -->
        <attr name="waveColor" />
        <!-- slide bar　选中时候波浪半径 -->
        <attr name="waveRadius" />
        <!-- slide bar　里面文字上＋下padding -->
        <attr name="contentPadding" />
        <!-- slide bar　上+下+右padding -->
        <attr name="barPadding" />
        <!-- slide bar　宽度 -->
        <attr name="barWidth" />
    </declare-styleable>

    <declare-styleable name="AZSideBarView">
        <!-- slide bar 有效内容背景 -->
        <attr name="backgroundColor" />
        <!-- slide bar　描边 -->
        <attr name="strokeColor" />
        <!-- slide bar 文字颜色 -->
        <attr name="textColor" />
        <!-- slide bar　文字大小 -->
        <attr name="textSize" />
        <!-- slide bar 选中文字颜色 -->
        <attr name="selectTextColor" />
        <!-- slide bar 选中文字大小 -->
        <attr name="selectTextSize" />
        <!-- slide bar　选中时候提示文字颜色 -->
        <attr name="hintTextColor" />
        <!-- slide bar　选中时候提示文字大小 -->
        <attr name="hintTextSize" />
        <!-- slide bar　选中时候提示圆形半径 -->
        <attr name="hintCircleRadius" />
        <attr name="hintShape" />
        <!-- slide bar　选中时候提示圆形颜色 -->
        <attr name="hintCircleColor" />
        <!-- slide bar　里面文字上＋下padding -->
        <attr name="contentPadding" />
        <!-- slide bar　上+下+右padding -->
        <attr name="barPadding" />
        <!-- slide bar　宽度 -->
        <attr name="barWidth" />
    </declare-styleable>


    <!--扩散圆颜色-->
    <attr name="diffuse_color" format="color"/>
    <!--中心圆颜色-->
    <attr name="diffuse_coreColor" format="color"/>
    <!--中心圆图片-->
    <attr name="diffuse_coreImage" format="reference"/>
    <!--中心圆半径-->
    <attr name="diffuse_coreRadius" format="float"/>
    <!--扩散圆宽度,值越小越宽-->
    <attr name="diffuse_width" format="integer"/>
    <!--最大扩散宽度-->
    <attr name="diffuse_maxWidth" format="integer"/>
    <!--扩散速度，值越大越快-->
    <attr name="diffuse_speed" format="integer"/>

    <declare-styleable name="DiffuseView">
        <attr name="diffuse_color"/>
        <attr name="diffuse_coreColor"/>
        <attr name="diffuse_coreImage"/>
        <attr name="diffuse_coreRadius"/>
        <attr name="diffuse_width"/>
        <attr name="diffuse_maxWidth"/>
        <attr name="diffuse_speed"/>
    </declare-styleable>


    <declare-styleable name="DYLoadingView">
        <attr name="radius1" format="dimension"/>
        <attr name="radius2" format="dimension"/>
        <attr name="rtlScale" format="float"/>
        <attr name="ltrScale" format="float"/>
        <attr name="color1" format="color"/>
        <attr name="color2" format="color"/>
        <attr name="mixColor" format="color"/>
        <attr name="duration" format="integer"/>
        <attr name="pauseDuration" format="integer"/>
        <attr name="gap" format="dimension"/>
        <attr name="scaleStartFraction" format="float"/>
        <attr name="scaleEndFraction" format="float"/>
    </declare-styleable>


    <style name="MyDialog" parent="android:style/Theme.Dialog">
        <!--背景颜色及和透明程度-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--是否去除标题 -->
        <item name="android:windowNoTitle">true</item>
        <!--是否去除边框-->
        <item name="android:windowFrame">@null</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsFloating">true</item>
        <!--是否模糊-->
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <declare-styleable name="TabMenu">
        <attr name="selected" format="integer" />
        <attr name="item_count" format="integer" />
        <attr name="icon_size" format="dimension" />
        <attr name="android:textSize" />
        <attr name="text_color" format="color" />
        <attr name="text_select_color" format="color" />
        <attr name="item_bg" format="reference|color" />
    </declare-styleable>
</resources>